{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "plugins": [
    "jsx-a11y",
    "nextjs",
    "import",
    "react",
    "typescript",
    "unicorn",
    "oxc"
  ],
  "env": {
    "builtin": true
  },
  "ignorePatterns": [
    "node_modules/**",
    ".next/**",
    "out/**",
    "build/**",
    "next-env.d.ts"
  ]
}
