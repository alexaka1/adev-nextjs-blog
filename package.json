{
  "name": "adev-nextjs-blog",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "check-site-meta": "bunx --bun check-site-meta --no-analytics http://localhost:3000",
    "dev": "next dev",
    "dev:webpack": "bun run --bun next dev --webpack",
    "dev:sw": "next dev --experimental-https",
    "build": "next build",
    "start": "bun run --bun next start",
    "lint": "bun run --bun oxlint --type-aware",
    "lint:fix": "bun run --bun oxlint --type-aware --fix",
    "fmt": "bun run --bun oxfmt --check",
    "fmt:fix": "bun run --bun oxfmt",
    "renovate:debug": "LOG_LEVEL=debug bunx --bun renovate --platform=local",
    "tw:upgrade": "bunx --bun @tailwindcss/upgrade@latest"
  },
  "overrides": {
    "brace-expansion": ">=2.0.2",
    "mdast-util-to-hast": ">=13.2.1"
  },
  "dependencies": {
    "@mdx-js/loader": "3.1.1",
    "@mdx-js/react": "3.1.1",
    "@next/mdx": "16.1.6",
    "@radix-ui/react-avatar": "1.1.11",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-navigation-menu": "1.2.14",
    "@radix-ui/react-slot": "1.2.4",
    "@sentry/nextjs": "10.38.0",
    "@t3-oss/env-nextjs": "0.13.10",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.563.0",
    "next": "16.1.6",
    "next-themes": "0.4.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "rehype-starry-night": "2.2.0",
    "remark-frontmatter": "5.0.0",
    "remark-gfm": "4.0.1",
    "remark-mdx-frontmatter": "5.2.0",
    "tailwind-merge": "3.4.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.18",
    "@types/mdx": "2.0.13",
    "@types/node": "24.10.9",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "eslint-plugin-import-zod": "1.2.1",
    "oxfmt": "0.28.0",
    "oxlint": "1.43.0",
    "oxlint-tsgolint": "0.11.4",
    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3"
  },
  "engines": {
    "bun": ">=1.3.4"
  },
  "engineStrict": true,
  "trustedDependencies": [
    "@sentry/cli",
    "unrs-resolver"
  ]
}
